<p id="notice"><%=notice %></p>

<h1>Businesses</h1>

<table>
  <thead>
    <tr>
        <th>Company Name </th>
        <th>Contact title</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Website</th>
        <th># Employees</th>
        <th>NAISC Code</th>
        <th>SIC Code</th>
        <th>Description</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th>Country</th>
        <th>Created</th>
        <th>Updated</th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.company_name %></td>
        <td><%= business.contact_title %></td>
        <td><%= business.contact_first_name%></td>
        <td><%= business.contact_last_name%></td>
        <td><%= business.phone %></td>
        <td><%= business.email %></td>
        <td><%= business.website %></td>
        <td><%= business.number_of_employees %></td>
        <td><%= business.naics_code %></td>
        <td><%= business.sic_code %></td>
        <td><%= business.description %></td>
        <td><%= business.address %></td>
        <td><%= business.city %></td>
        <td><%= business.state %></td>
        <td><%= business.postal_code %></td>
        <td><%= business.country %></td>
        <td><%= business.created_at %></td>
        <td><%= business.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%=link_to 'New Business', new_business_path %>
