<div class="row">
  <div class="col s12">
    <%= simple_form_for([:admin, @verification] ) do |f| %>
      <%= f.error_notification %>
      <%= f.input :assigned_user, collection: User.with_role(:admin).collect {|u| [u.name, u.id ] }  %>
      <%= f.submit 'Update'  %>
    <% end %>
  </div>
</div>
