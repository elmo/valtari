<h1>Deal Room User Profile</h1>

<div class="row">
  <div class="col s12">
      <h3><%= @user.email -%></h3>
      <h4><%= @user.full_name -%></h4>
      <%= image_tag @user.avatar.url if @user.avatar.url  -%>
  </div>
</div>

<div class="row">
  <div class="col s12">
    <%= render partial:  '/private/deal_rooms/owners/user_uploads', locals: { user: @user}  %>
  </div>
</div>

<div class="row">
  <div class="col s12">
     <h3>Activity Log<h3>
  </div>
</div>

<div class="row">
  <div class="col s12">
     <h3>Comments<h3>
  </div>
</div>

<div class="row">
  <div class="col s12">
     <h3>Notes<h3>
  </div>
</div>

<div class="row">
  <div class="col s12">
     <h3>Todos<h3>
  </div>
</div>
