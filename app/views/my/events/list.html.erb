<div class="row">
  <div class="col s8 m8">
    <h1>Events</h1>
     <ul>
      <% @events.each do |event| %>
        <li>
         <p><%= event_time_and_date(event) %></p>
          <h3> <%= link_to event.title, event_path(event)  %></h3>
             <%= event_photo(event, size: :small) -%>
          <p> <%= event.description -%></p>
        </li>
      <% end %>
    </ul>
    <p><%= link_to 'calendar view', my_events_path -%></p>
    <p><%= link_to 'new event', new_my_event_path -%></p>
  </div>
</div>
