<div class="container">
  <div class="row">
    <div class="col s12 l6 offset-l2">
      <div class="card">
        <div class="card-content">
          <h5 class="header-five">Events</h5>
            <ul>
              <% @events.each do |event| %>
              <li>
                <strong> <%= link_to event.title, event_path(event)  %></strong>
                <p><%= event_time_and_date(event) %></p>
                <%= event_photo(event, size: :small) -%>
                <br>

              </li>
            <% end %>
            </ul>
                <p><%= link_to 'calendar view', my_events_path -%></p>
    <p><%= link_to 'new event', new_my_event_path -%></p>
  </div>
</div>
