<%= simple_form_for([:my, @event]) do |f| %>
  <%= f.error_notification %>
    <%= f.input :title %>
    <%= f.input :start_time, as: :string, input_html: {class: 'datepicker'}  %>
    <%= f.input :end_time, as: :string,  input_html: {class: 'datepicker' }   %>
    <%= f.button :submit %>
<% end %>
