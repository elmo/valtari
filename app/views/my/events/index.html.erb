<div class="row">
  <div class="col s8 m8">
    <h1>Events</h1>
      <%= month_calendar events: @events do |date, events| %>
      <%= date %>
      <% events.each do |event| %>
        <div>
          <%= link_to event.title,  event_path(event)  %>
         </div>
       <% end %>
      <% end %>
    </div>
  </div>
  <p><%= link_to 'list view', my_events_path(view: 'list') -%></p>
  <p><%= link_to 'new event', new_my_event_path -%></p>
</div>
